{
  "name": "Portfolio",
  "version": "1.0.0",
  "description": "Landing page for Code by Marius",
  "browserlist": "last 10 versions",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss ../../static/css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss ../../static/css/style.comp.css",
    "prefix:css": "postcss ../../static/css/style.comp.css --use autoprefixer -o ../../static/css/style.prefix.css",
    "compress:css": "node-sass ../../static/css/style.prefix.css ../../static/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "author": "Marius",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "concat": "^1.0.3",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^4.1.1"
  },
  "dependencies": {
    "animate.css": "^4.1.1"
  }
}
